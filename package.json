{
  "name": "jupyterlab-theme-toggle",
  "version": "0.4.2",
  "description": "JupyterLab extension to toggle the theme in the Top Bar area",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "http://github.com/jtpio/jupyterlab-theme-toggle",
  "bugs": {
    "url": "http://github.com/jtpio/jupyterlab-theme-toggle/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Jeremy Tuloup",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "http://github.com/jtpio/jupyterlab-theme-toggle.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "prepare": "npm run clean && npm run build",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@blueprintjs/core": "^3.15.1",
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/apputils": "^2.0.0",
    "jupyterlab-topbar": "^0.5.0",
    "react": "^16.9.1"
  },
  "devDependencies": {
    "@types/react": "^16.9.1",
    "rimraf": "^3.0.2",
    "typescript": "~3.8.1"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  }
}
